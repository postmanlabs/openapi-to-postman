{
	"openapi": "3.0.0",
	"info": {
		"version": "1.0.0",
		"title": "Swagger Petstore"
	},
	"servers": [
		{
			"url": "http://petstore.swagger.io/v1"
		}
	],
	"paths": {
		"/pets": {
			"get": {
				"responses": {
					"200": {
						"description": "Successfull",
						"content": {
							"application/json": {
								"schema": {
									"type": "array",
									"items": {
										"type": "object",
										"properties": {
											"id": {
												"type": "integer",
												"format": "int64",
												"readOnly": true
											},
											"tag": {
												"type": "string",
												"writeOnly": true
											}
										}
									}
								}
							}
						}
					}
				}
			},
			"post": {
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/Pet"
							}
						}
					}
				},
				"responses": {
					"200": {
						"description": "Successfully created a pet"
					}
				}
			}
		}
	},
	"components": {
		"schemas": {
			"Pet": {
				"properties": {
					"id": {
						"type": "integer",
						"format": "int64",
						"readOnly": true
					},
					"name": {
						"type": "string"
					},
					"tag": {
						"type": "string",
						"writeOnly": true
					}
				}
			}
		}
	}
}
